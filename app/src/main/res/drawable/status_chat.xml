<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="oval">
            <solid android:color="@color/light_green_500" />
            <stroke
                android:width="1dp"
                android:color="@color/white" />
            <size
                android:width="12dp"
                android:height="12dp" />
        </shape>
    </item>
</layer-list>